{
  "name": "poodl",
  "version": "1.0.0",
  "description": "Senior Center App",
  "main": "server.js",
  "scripts": {
    "client-install": "yarn --cwd client",
    "start": "node server.js",
    "test": "yarn --cwd client test",
    "server": "nodemon server.js",
    "client": "yarn --cwd client start",
    "start:dev": "concurrently \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0"
  }
}
